package com.nisum;

import java.util.Scanner;

public class A16 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
  Scanner sc=new Scanner(System.in);
  System.out.println("Enter the first String:");
  String s3=sc.next();
  StringBuffer s1=new StringBuffer(s3);
  System.out.println("Enter the second String:");
  String s4=sc.next();
  StringBuffer s2=new StringBuffer(s4);
   int len1=s1.length();
   int len2=s2.length();
   int t=len1+len2;
   s1=s2.append(s1);
   System.out.println(s1);
   
   for(int i=0;i<len2;i++)
   {
	   s2=s2.append(s1.charAt(i));
   }
   for(int i=len2-1;i>t;i++)
   {
	   s1=s1.append(s1.charAt(i));
   }
   
 //  s2=s1.substring(0,len1);
   //s1=s1.substring(len1, t);

   
   System.out.println("Swapped string values:"+s2);
	}

}
