package com.nisum;

public class A7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
 int a1[]= {1,2,3};
 int a2[]= {3,4,5};
 int l1=a1.length;
 int l2=a2.length;
 int res[]=new int[l1+l2];
 for(int i=0;i<l1;i++)
 {
	 res[i]=a1[i];
 }
 for(int j=l1;j<l2;j++)
 { 
	 int m=0;
	 res[j]=a2[m];
	 m++;
	 //System.out.print(res[j]+" ");
 }
 for(int k=0;k<l1+l2;k++)
 {
	 System.out.print(res[k]+" ");
 }
	}

}
