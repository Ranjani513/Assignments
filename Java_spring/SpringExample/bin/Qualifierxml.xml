<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:mvc="http://www.springframework.org/schema/mvc"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-2.5.xsd" >

    <context:annotation-config />
  <bean id="personqualifier" class="com.Qualifier.PersonQualifier"/>  
<bean id="Person1" class="com.Qualifier.Person" name="Person1" >  
<property name="name" value="ranjani"></property>  
<property name="age" value="21"></property>  
</bean>  
  <bean id="Person2" class="com.Qualifier.Person" name="Person2">  
<property name="name" value="Divya"></property>  
<property name="age" value="20"></property>  
</bean>  

</beans>  