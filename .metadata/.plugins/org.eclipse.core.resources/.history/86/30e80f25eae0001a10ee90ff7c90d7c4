package com.nisum;

public class A25 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int count=0,rem=0,sum=0,c;
		int n=10;
   outer:
	   while(n>10) {
		  c=n;
		   inner:
			   count=0;
			   while(n!=0)
			   {
				   rem=n%10;
				   sum=sum*10+rem*3;
				   n/=10;
			   }
		  if(count<=10) {
		 
		  if(c==n)
		  {
			  count++;
			  System.out.println(c);
		  }
		 
		  
	   }
		  else
		  {
			  break outer;
		  }
		  n++;
	   }
	}

}
